<%- include("partials/header") -%>

<h1>Home</h1>
<p><%= homeStartingContent %></p>

<% posts.forEach(function(post) { %>
    <h1><%= post.title %></h1>
    <p><%= truncate.truncateString(post.content, 100); %>
    <a href="/posts/<%= _.lowerCase(post.title); %>">Read more.</a></p>
<% }); %>

<%- include("partials/footer") -%>